def check_telegram_user():
    """–ü—Ä–æ–≤–µ—Ä–∫–∞ —Å—É—â–µ—Å—Ç–≤–æ–≤–∞–Ω–∏—è –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è –≤ Telegram"""
    clear_screen()
    print_banner()
    print(f"\n{Fore.GREEN}[7] {Fore.WHITE}–ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è –≤ Telegram")
    print(f"{Fore.CYAN}="*50)
    
    username = input(f"{Fore.YELLOW}–í–≤–µ–¥–∏—Ç–µ —é–∑–µ—Ä–Ω–µ–π–º (—Å @ –∏–ª–∏ –±–µ–∑): ").strip().replace("@", "")
    
    if not username:
        print(f"{Fore.RED}‚ùå –í—ã –Ω–µ –≤–≤–µ–ª–∏ —é–∑–µ—Ä–Ω–µ–π–º!")
        return
    
    url = f"https://t.me/{username}"
    headers = {'User-Agent': UserAgent().random}
    
    try:
        print(f"\n{Fore.YELLOW}üîç –ü—Ä–æ–≤–µ—Ä—è–µ–º @{username}...")
        response = requests.get(url, headers=headers, timeout=10)
        
        if response.status_code == 200:
            if "tgme_username_link" in response.text or "tgme_page_title" in response.text:
                print(f"\n{Fore.GREEN}‚úÖ –ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å @{username} —Å—É—â–µ—Å—Ç–≤—É–µ—Ç!")
                print(f"{Fore.CYAN}="*50)
                print(f"{Fore.WHITE}üåê –°—Å—ã–ª–∫–∞ –Ω–∞ –∞–∫–∫–∞—É–Ω—Ç: {Fore.CYAN}{url}")
                print(f"{Fore.CYAN}="*50)
            else:
                print(f"\n{Fore.RED}‚ùå –ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å @{username} –Ω–µ –Ω–∞–π–¥–µ–Ω!")
        else:
            print(f"\n{Fore.RED}‚ùå –û—à–∏–±–∫–∞ –∑–∞–ø—Ä–æ—Å–∞ (–∫–æ–¥ {response.status_code})")
            
    except requests.ConnectionError:
        print(f"\n{Fore.RED}‚ö† –û—à–∏–±–∫–∞ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏—è –∫ –∏–Ω—Ç–µ—Ä–Ω–µ—Ç—É")
    except Exception as e:
        print(f"\n{Fore.RED}‚ö† –û—à–∏–±–∫–∞: {str(e)}")

def ip_lookup():
    """–ü–æ–∏—Å–∫ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –ø–æ IP-–∞–¥—Ä–µ—Å—É"""
    clear_screen()
    print_banner()
    print(f"\n{Fore.GREEN}[8] {Fore.WHITE}–ü–æ–∏—Å–∫ –ø–æ IP-–∞–¥—Ä–µ—Å—É")
    print(f"{Fore.CYAN}="*50)
    
    def get_ip_info(ip):
        url = f"http://ip-api.com/json/{ip}?fields=status,message,country,city,org,timezone,query"
        headers = {"User-Agent": UserAgent().random}
        
        try:
            response = requests.get(url, headers=headers, timeout=10)
            data = response.json()
            
            if data.get("status") != "success":
                error = data.get("message", "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –æ—à–∏–±–∫–∞")
                print(f"{Fore.RED}‚ùå –û—à–∏–±–∫–∞: {error}")
                return None
            
            timezone = data.get("timezone", "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ")
            local_time = "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ"
            if timezone != "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ":
                try:
                    tz = pytz.timezone(timezone)
                    local_time = datetime.now(tz).strftime("%Y-%m-%d %H:%M:%S")
                except pytz.UnknownTimeZoneError:
                    pass
            
            return {
                "üåê IP-–∞–¥—Ä–µ—Å": data.get("query", ip),
                "üè≥Ô∏è –°—Ç—Ä–∞–Ω–∞": data.get("country", "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ"),
                "üèôÔ∏è –ì–æ—Ä–æ–¥": data.get("city", "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ"),
                "üè¢ –û—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏—è": data.get("org", "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ"),
                "‚è∞ –ß–∞—Å–æ–≤–æ–π –ø–æ—è—Å": timezone,
                "üïí –ú–µ—Å—Ç–Ω–æ–µ –≤—Ä–µ–º—è": local_time,
            }
        
        except requests.exceptions.RequestException as e:
            print(f"{Fore.RED}üö´ –û—à–∏–±–∫–∞ –ø—Ä–∏ –∑–∞–ø—Ä–æ—Å–µ: {e}")
            return None

    print(f"{Fore.YELLOW}üîç –í–≤–µ–¥–∏—Ç–µ IP-–∞–¥—Ä–µ—Å (–Ω–∞–ø—Ä–∏–º–µ—Ä: 11.111.111.111):")
    ip = input().strip()
    
    if not re.match(r"^(\d{1,3}\.){3}\d{1,3}$", ip):
        print(f"{Fore.RED}‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç IP! –ü—Ä–∏–º–µ—Ä: 11.111.111.111")
        return
    
    print(f"\n{Fore.YELLOW}‚åõ –ó–∞–ø—Ä–∞—à–∏–≤–∞—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é...\n")
    
    info = get_ip_info(ip)
    if info:
        print(f"{Fore.CYAN}üìã –†–µ–∑—É–ª—å—Ç–∞—Ç—ã –ø–æ–∏—Å–∫–∞ –ø–æ IP:")
        print(f"{Fore.CYAN}="*50)
        for key, value in info.items():
            print(f"{Fore.WHITE}{key}: {Fore.YELLOW}{value}")
        print(f"{Fore.CYAN}="*50)
    else:
        print(f"{Fore.RED}‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å –¥–∞–Ω–Ω—ã–µ. –ü—Ä–æ–≤–µ—Ä—å—Ç–µ IP –∏ –∏–Ω—Ç–µ—Ä–Ω–µ—Ç.")

def check_my_ip():
    """–ü—Ä–æ–≤–µ—Ä–∫–∞ —Ç–µ–∫—É—â–µ–≥–æ IP-–∞–¥—Ä–µ—Å–∞ –∏ VPN —Å—Ç–∞—Ç—É—Å–∞"""
    clear_screen()
    print_banner()
    print(f"\n{Fore.GREEN}[9] {Fore.WHITE}–ü—Ä–æ–≤–µ—Ä–∏—Ç—å –º–æ–π IP")
    print(f"{Fore.CYAN}="*50)
    
    def get_local_ip():
        try:
            hostname = socket.gethostname()
            local_ip = socket.gethostbyname(hostname)
            return local_ip
        except:
            return "–ù–µ —É–¥–∞–ª–æ—Å—å –æ–ø—Ä–µ–¥–µ–ª–∏—Ç—å"
    
    def get_ip_info(use_vpn_check=True):
        try:
            # –ü–æ–ª—É—á–∞–µ–º —Ç–µ–∫—É—â–∏–π IP
            response = requests.get('https://api.ipify.org?format=json', timeout=5)
            current_ip = response.json()['ip']
            
            # –ü–æ–ª—É—á–∞–µ–º –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ–± IP
            ip_info = requests.get(f'http://ip-api.com/json/{current_ip}?fields=66846719', timeout=5).json()
            
            # –ü—Ä–æ–≤–µ—Ä—è–µ–º VPN/Proxy
            vpn_detected = False
            protection_level = "–°—Ä–µ–¥–Ω—è—è"
            vpn_type = "–ù–µ –æ–±–Ω–∞—Ä—É–∂–µ–Ω"
            
            if use_vpn_check:
                # –ü—Ä–æ–≤–µ—Ä—è–µ–º –ø—Ä–∏–∑–Ω–∞–∫–∏ VPN/Proxy
                if ip_info.get('proxy') or ip_info.get('hosting'):
                    vpn_detected = True
                    vpn_type = "VPN/Proxy –æ–±–Ω–∞—Ä—É–∂–µ–Ω"
                    protection_level = "–í—ã—Å–æ–∫–∞—è"
                elif ip_info.get('mobile'):
                    vpn_type = "–ú–æ–±–∏–ª—å–Ω–æ–µ —Å–æ–µ–¥–∏–Ω–µ–Ω–∏–µ"
                    protection_level = "–ù–∏–∑–∫–∞—è"
                else:
                    vpn_type = "–ü—Ä—è–º–æ–µ —Å–æ–µ–¥–∏–Ω–µ–Ω–∏–µ"
                    protection_level = "–°—Ä–µ–¥–Ω—è—è"
            
            return {
                'current_ip': current_ip,
                'isp': ip_info.get('isp', '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'),
                'org': ip_info.get('org', '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'),
                'country': ip_info.get('country', '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'),
                'city': ip_info.get('city', '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'),
                'vpn_detected': vpn_detected,
                'vpn_type': vpn_type,
                'protection_level': protection_level
            }
            
        except Exception as e:
            return {'error': str(e)}
    
    # –õ–æ–∫–∞–ª—å–Ω—ã–π IP
    print(f"\n{Fore.WHITE}üñ•Ô∏è –í–∞—à –ª–æ–∫–∞–ª—å–Ω—ã–π IP: {Fore.YELLOW}{get_local_ip()}")
    
    # –¢–µ–∫—É—â–µ–µ —Å–æ–µ–¥–∏–Ω–µ–Ω–∏–µ
    print(f"\n{Fore.YELLOW}‚åõ –ê–Ω–∞–ª–∏–∑ —Ç–µ–∫—É—â–µ–≥–æ —Å–æ–µ–¥–∏–Ω–µ–Ω–∏—è...\n")
    info = get_ip_info()
    
    if 'error' in info:
        print(f"{Fore.RED}‚ùå –û—à–∏–±–∫–∞: {info['error']}")
    else:
        print(f"{Fore.CYAN}üìã –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ –≤–∞—à–µ–º IP:")
        print(f"{Fore.CYAN}="*50)
        print(f"{Fore.WHITE}üåê –í–Ω–µ—à–Ω–∏–π IP: {Fore.YELLOW}{info['current_ip']}")
        print(f"{Fore.WHITE}üì° –ü—Ä–æ–≤–∞–π–¥–µ—Ä: {Fore.YELLOW}{info['isp']}")
        print(f"{Fore.WHITE}üè¢ –û—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏—è: {Fore.YELLOW}{info['org']}")
        print(f"{Fore.WHITE}üåç –°—Ç—Ä–∞–Ω–∞: {Fore.YELLOW}{info['country']}")
        print(f"{Fore.WHITE}üèôÔ∏è –ì–æ—Ä–æ–¥: {Fore.YELLOW}{info['city']}")
        print(f"{Fore.WHITE}üõ°Ô∏è –°—Ç–∞—Ç—É—Å VPN: {Fore.YELLOW}{info['vpn_type']}")
        print(f"{Fore.WHITE}üîí –£—Ä–æ–≤–µ–Ω—å –∑–∞—â–∏—Ç—ã: {Fore.YELLOW}{info['protection_level']}")
        print(f"{Fore.CYAN}="*50)
    
    # –°—Ä–∞–≤–Ω–µ–Ω–∏–µ —Å VPN –∏ –±–µ–∑
    choice = input(f"\n{Fore.YELLOW}–•–æ—Ç–∏—Ç–µ —Å—Ä–∞–≤–Ω–∏—Ç—å —Å–æ–µ–¥–∏–Ω–µ–Ω–∏–µ —Å VPN –∏ –±–µ–∑? (y/n): ").lower()
    if choice == 'y':
        try:
            print(f"\n{Fore.YELLOW}‚åõ –ü—Ä–æ–≤–µ—Ä–∫–∞ –±–µ–∑ VPN...")
            normal_info = get_ip_info()
            
            input(f"\n{Fore.YELLOW}–ü–æ–¥–∫–ª—é—á–∏—Ç–µ VPN –∏ –Ω–∞–∂–º–∏—Ç–µ Enter –¥–ª—è –ø—Ä–æ–¥–æ–ª–∂–µ–Ω–∏—è...")
            
            print(f"\n{Fore.YELLOW}‚åõ –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å VPN...")
            vpn_info = get_ip_info()
            
            print(f"\n{Fore.CYAN}üìä –†–µ–∑—É–ª—å—Ç–∞—Ç—ã —Å—Ä–∞–≤–Ω–µ–Ω–∏—è:")
            print(f"{Fore.CYAN}="*50)
            print(f"{Fore.WHITE}üîπ –ë–µ–∑ VPN:")
            print(f"{Fore.WHITE}IP: {Fore.YELLOW}{normal_info['current_ip']}")
            print(f"{Fore.WHITE}–°—Ç—Ä–∞–Ω–∞: {Fore.YELLOW}{normal_info['country']}")
            print(f"{Fore.WHITE}–ì–æ—Ä–æ–¥: {Fore.YELLOW}{normal_info['city']}")
            print(f"{Fore.WHITE}–°—Ç–∞—Ç—É—Å VPN: {Fore.YELLOW}{normal_info['vpn_type']}")
            
            print(f"\n{Fore.WHITE}üî∏ –° VPN:")
            print(f"{Fore.WHITE}IP: {Fore.YELLOW}{vpn_info['current_ip']}")
            print(f"{Fore.WHITE}–°—Ç—Ä–∞–Ω–∞: {Fore.YELLOW}{vpn_info['country']}")
            print(f"{Fore.WHITE}–ì–æ—Ä–æ–¥: {Fore.YELLOW}{vpn_info['city']}")
            print(f"{Fore.WHITE}–°—Ç–∞—Ç—É—Å VPN: {Fore.YELLOW}{vpn_info['vpn_type']}")
            print(f"{Fore.WHITE}–£—Ä–æ–≤–µ–Ω—å –∑–∞—â–∏—Ç—ã: {Fore.YELLOW}{vpn_info['protection_level']}")
            print(f"{Fore.CYAN}="*50)
        except Exception as e:
            print(f"{Fore.RED}‚ùå –û—à–∏–±–∫–∞ –ø—Ä–∏ —Å—Ä–∞–≤–Ω–µ–Ω–∏–∏: {e}")
    
    print(f"\n{Fore.GREEN}‚úÖ –ü—Ä–æ–≤–µ—Ä–∫–∞ –∑–∞–≤–µ—Ä—à–µ–Ω–∞!")

def exit_program():
    """–í—ã—Ö–æ–¥ –∏–∑ –ø—Ä–æ–≥—Ä–∞–º–º—ã"""
    print(f"\n{Fore.GREEN}üñ§ –°–ø–∞—Å–∏–±–æ –∑–∞ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ Dark Telegram! –î–æ —Å–≤–∏–¥–∞–Ω–∏—è!{Style.RESET_ALL}")
    sys.exit()

def main_menu():
    """–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é"""
    while True:
        clear_screen()
        print_banner()
        
        print(f"\n{Fore.MAGENTA}–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é:")
        print(f"{Fore.GREEN}[1] {Fore.WHITE}–ü–æ–∏—Å–∫ –ø–æ –Ω–æ–º–µ—Ä—É")
        print(f"{Fore.GREEN}[2] {Fore.WHITE}–°–ø–∞–º –∫–æ–¥–∞–º–∏")
        print(f"{Fore.GREEN}[3] {Fore.WHITE}–†–∞—Å—à–∏—Ä–µ–Ω–Ω—ã–π –ø–æ–∏—Å–∫")
        print(f"{Fore.GREEN}[4] {Fore.WHITE}–ì–µ–Ω–µ—Ä–∞—Ç–æ—Ä –ø–∞—Ä–æ–ª–µ–π")
        print(f"{Fore.GREEN}[5] {Fore.WHITE}–†–∞–∑—Ä–∞–±–æ—Ç—á–∏–∫")
        print(f"{Fore.GREEN}[6] {Fore.WHITE}–ü–æ–¥–¥–µ—Ä–∂–∞—Ç—å —Ä–∞–∑—Ä–∞–±–æ—Ç—á–∏–∫–∞")
        print(f"{Fore.GREEN}[7] {Fore.WHITE}–ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è –≤ TG")
        print(f"{Fore.GREEN}[8] {Fore.WHITE}–ü–æ–∏—Å–∫ –ø–æ IP")
        print(f"{Fore.GREEN}[9] {Fore.WHITE}–ü—Ä–æ–≤–µ—Ä–∏—Ç—å –º–æ–π IP")
        print(f"{Fore.GREEN}[10] {Fore.WHITE}–í—ã—Ö–æ–¥")
        
        choice = input(f"\n{Fore.YELLOW}–í—ã–±–µ—Ä–∏—Ç–µ –æ–ø—Ü–∏—é (1-10): ").strip()
        
        if choice == "1":
            phone_lookup()
        elif choice == "2":
            test_attack()
        elif choice == "3":
            advanced_phone_lookup()
        elif choice == "4":
            generate_password()
        elif choice == "5":
            show_credits()
        elif choice == "6":
            show_donate()
        elif choice == "7":
            check_telegram_user()
        elif choice == "8":
            ip_lookup()
        elif choice == "9":
            check_my_ip()
        elif choice == "10":
            exit_program()
        else:
            print(f"{Fore.RED}‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä. –ü–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.")
        
        input(f"\n{Fore.YELLOW}–ù–∞–∂–º–∏—Ç–µ ENTER —á—Ç–æ–±—ã –ø—Ä–æ–¥–æ–ª–∂–∏—Ç—å...")

if __name__ == "__main__":
    # –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–∞—Ä–æ–ª—è (–î–û–ë–ê–í–õ–ï–ù–û)
    clear_screen()
    print_banner()
    password_input = input(f"{Fore.YELLOW}–í–≤–µ–¥–∏—Ç–µ –ø–∞—Ä–æ–ª—å –¥–ª—è –¥–æ—Å—Ç—É–ø–∞: ").strip()
    if password_input != PASSWORD:
        print(f"{Fore.RED}‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π –ø–∞—Ä–æ–ª—å! –í—ã—Ö–æ–¥...")
        sys.exit()
    print(f"{Fore.GREEN}‚úÖ –î–æ—Å—Ç—É–ø —Ä–∞–∑—Ä–µ—à–µ–Ω!")
    input(f"{Fore.YELLOW}–ù–∞–∂–º–∏—Ç–µ ENTER —á—Ç–æ–±—ã –ø—Ä–æ–¥–æ–ª–∂–∏—Ç—å...")
    main_menu()  # –ó–∞–ø—É—Å–∫ –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –º–µ–Ω—é
